{
  "author": "Alex Palumbo",
  "manifest_version": 2,
  "name": "Patreon Pop-up Video Player",
  "description": "Allows users to watch videos in a resizable popup window while remaining on Patreon's website.",
  "version": "1.6",
  "content_scripts": [
    {
      "matches": [
        "*://*.patreon.com/*"
      ],
      "run_at": "document_end",
      "js": ["js/jquery-3.3.1.min.js","js/content.js", "js/jquery-ui.js", "js/conversionFunctions.js"],
      "css": ["css/index.css", "css/jquery-ui.css"]
    }
  ],
  "web_accessible_resources":["images/popup.png", "images/closeIconWhite.png", "images/frameResize.png"],
  "icons": { "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}